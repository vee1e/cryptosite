RewriteEngine On

# Handle privacy-policy route
RewriteRule ^privacy-policy/?$ /dist/privacy-policy/index.html [L]

# Handle alumni route
RewriteRule ^alumni/?$ /dist/alumni/index.html [L]

# Handle other nested routes if needed
RewriteRule ^([^/]+)/?$ /dist/$1/index.html [L]

# Fallback to main index.html
RewriteRule ^$ /index.html [L]
